{
    "name": "pradeep-sati12/password-changed-notification-mail",
    "description": "mail send when ever password changed",
    "keywords": [
        "Pradeep-sati12",
        "laravel",
        "password-changed-notification-mail"
    ],
    "homepage": "https://github.com/pradeep-sati12/password-changed-notification-mail",
    "license": "MIT",
    "authors": [
        {
            "name": "Pradeep Sati",
            "email": "pradeep.sati@velsof.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Pradeep-sati12\\PasswordChangedNotificationMail\\": "src",
            "Pradeep-sati12\\PasswordChangedNotificationMail\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pradeep-sati12\\PasswordChangedNotificationMail\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pradeep-sati12\\PasswordChangedNotificationMail\\PasswordChangedNotificationMailServiceProvider"
            ],
            "aliases": {
                "PasswordChangedNotificationMail": "Pradeep-sati12\\PasswordChangedNotificationMail\\Facades\\PasswordChangedNotificationMail"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
